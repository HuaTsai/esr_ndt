{
    // See https://go.microsoft.com/fwlink/?LinkId=733558 
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build",
            "type": "catkin_build",
            "task": "build",
            "problemMatcher": [
                "$catkin-gcc",
                "$catkin-cmake"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            }
        },
        {
            "label": "vio & ego motion",
            "type": "shell",
            "command": ".vscode/scripts/run vio_ego"
        },
        {
            "label": "run",
            "type": "shell",
            "command": ".vscode/scripts/run run"
        },
        {
            "label": "map_build",
            "type": "shell",
            "command": ".vscode/scripts/run map_build"
        }
    ]
}